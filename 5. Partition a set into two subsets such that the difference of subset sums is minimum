import java.io.*;

class GFG {
	public static void main (String[] args) {
		System.out.println("GfG!");
		int[] arr = {1, 6, 11, 5 };
		int sum = 0;
		for(int i=0; i< arr.length; i++){
		 sum +=arr[i];   
		}
		System.out.println("count of subset! " + difference_of_subset_sums_is_minimum(arr, sum));
	}
	public static int difference_of_subset_sums_is_minimum(int[] nums, int sum){
        int[][] t = new int[nums.length+1][sum+1];
        for(int i=0; i< nums.length+1; i++){
            for(int j=0; j<sum+1; j++){
                if(i==0){
                    t[i][j] = 0;
                }
                if(j==0){
                    t[i][j] = 1;
                }
            }
        }
        for(int i=1; i< nums.length+1; i++){
            for(int j=1; j<sum+1; j++){
                if(nums[i-1] <= j){
                    t[i][j] = t[i-1][j-nums[i-1]] | t[i-1][j];
                }
                else
                t[i][j] = t[i-1][j];
            }
        }
        //return t[nums.length][sum];
        int min = Integer.MAX_VALUE;
        for(int i=0; i< nums.length; i++){
            min = Math.abs(Math.min(min, sum - 2*nums[i]));
        }
        return min;
	}
}//output: 4
//https://ide.geeksforgeeks.org/online-java-compiler/d2b07170-f3e1-4863-834b-c19b519455ad
